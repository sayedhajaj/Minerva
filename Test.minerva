var arr = [1, 2, 3];

function sumList(arr) => {
    var total = 0;
    var index = 0;
    while (index < arr.length) {
        total = total + arr[index];
        index = index + 1;
    };
    total;
};

function reduce(list, reducer, initialValue) => {
    var result = initialValue;
    var index = 0;
    while (index < list.length) {
        var current = list[index];
        result = reducer(result, current);
        index = index + 1;
    };
    result;
};

function addReducer(acc, item) => acc * item;

//print reduce(arr, addReducer, 1);
print arr.reduce(addReducer, 1);

function printList(list) => {
    print "[";
    var index = 0;
    while (index < list.length) {
        print list[index];
        index = index + 1;
    };
    print "]";
};

