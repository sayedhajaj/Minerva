class Parent(var name: String) {
    function getGreeting() => {
        this.name + " please";
    };
};

class Child extends Parent("Jack") {
    var age = 2;
    function test() => super.getNum() + 2;
};

function createParent(name: String) => Parent(name);

var jack: Child = Child();
print jack.getGreeting() + "!";

