class Parent(var name: String) {
    function getGreeting() => {
        this.name + " please";
    };
};

class Child extends Parent("Jack") {
    var age = 2;
    function test() => super.getNum() + 2;
};


function foo(lambda: (Int)=>Int, param: Int) => lambda(param);

print foo(function(num: Int) => num*2, 5);

var blah: String | Parent | Int[] = "hmm";

print typematch(blah) {
    Int => blah * 2;
    String => blah;
    Parent => blah.name;
    Int[] => blah[0];
    (Int)=>Boolean => blah(2);
};

